		}
		.thumbnail > img,
		.thumbnail a > img {
		  margin-right: auto;
		  margin-left: auto;
		}
		a.thumbnail:hover,
		a.thumbnail:focus,
		a.thumbnail.active {
		  border-color: #337ab7;
		}
		.thumbnail .caption {
		  padding: 9px;
		  color: #333;
		}

